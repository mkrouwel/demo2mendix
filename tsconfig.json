{
  "compilerOptions": {
    "target": "ES2021",
    "lib": ["ES2021"],
    "module": "Node16",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "noImplicitThis": true,
    "rootDir": "./src",
    "outDir": "./build"
  },"include": ["src", "types"]
}